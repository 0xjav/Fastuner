# Fastuner Environment Configuration

# Database
DATABASE_URL=sqlite:///./fastuner.db

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# S3 Buckets
S3_BUCKET_DATASETS=fastuner-datasets-dev
S3_BUCKET_MODELS=fastuner-models-dev
S3_BUCKET_ARTIFACTS=fastuner-artifacts-dev

# SageMaker
SAGEMAKER_EXECUTION_ROLE_ARN=arn:aws:iam::123456789012:role/SageMakerExecutionRole
SAGEMAKER_VPC_ID=vpc-xxxxx
SAGEMAKER_SUBNET_IDS=subnet-xxxxx,subnet-yyyyy
SAGEMAKER_SECURITY_GROUP_IDS=sg-xxxxx

# Cognito (for authentication)
COGNITO_USER_POOL_ID=us-east-1_xxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxx
COGNITO_REGION=us-east-1

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
LOG_LEVEL=INFO

# Deployment TTL (in seconds)
DEFAULT_DEPLOYMENT_TTL=3600

# Environment
ENVIRONMENT=development
